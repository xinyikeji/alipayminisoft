/*不使用回优惠券*/
.no-coupons-bth {
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	color: #fff;
	background: #1bbc9b;

	border-top: 1px solid #ededed;
	box-shadow: 0 0 7px 3px rgba(0, 0, 0, 0.06);
}

.active-jiaobiao {
	position: absolute;
	right: 0;
	top: -8rpx;
	font-size: 40rpx;
}

.xy-optcoupons-item .xy-optcoupons-info {
	display: flex;
	justify-content: space-between;
	padding: 10px 15px;
	background: #ffffff;
	border-radius: 0 0 8px 8px;
}

.xy-optcoupons-item .xy-optcoupons-info .title {
	font-size: 14px;
	color: #000000;
	font-weight: bold;
}
.xy-optcoupons-item .xy-optcoupons-info .type {
	padding: 5px 0;
}
.xy-optcoupons-item .xy-optcoupons-info .type text {
	border: 1px solid #ededed;
	padding: 5px;
	color: #666666;
	font-size: 8px;
}

.xy-optcoupons-item .xy-optcoupons-info .validity {
	font-size: 12px;
	color: #666666;
}

.xy-optcoupons-right {
	justify-content: center;
	justify-items: center;
	align-items: center;

	align-content: center;
}

.xy-coupons-price {
	display: flex;
	justify-items: flex-end;
	justify-content: flex-end;
	align-items: flex-end;
	align-content: flex-end;
}

.xy-optcoupons-right .full {
	color: #666666;
	font-size: 12px;
}

.xy-optcoupons-explain {
	background: #ffffff;
	font-size: 10px;
	color: #999999;
	padding: 10px 15px;
	border-top: 2px dashed #f8f8f8;
	border-radius: 8px 8px 0 0;
}

/*价格 START*/
.price {
	display: flex;
	color: red;
}
.yprice {
	display: flex;
	font-size: 10px;
	color: #999999;
	text-decoration: line-through solid #999999;
	margin-top: -10rpx;
}
.symbol {
	align-self: flex-end;
	padding-bottom: 8px;
}
.amount {
	font-size: 30px;
}
/*价格 END*/
