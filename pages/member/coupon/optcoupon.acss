.weui-media-box {
	padding: 0;
	display: flex;
	/*justify-content:center;*/
	/*flex-direction: row;*/
	align-items: stretch;
}

.weui-media-box__hd {
	color: #ffffff;
	padding: 24rpx 0;
	height: 208rpx;
	width: 245rpx;
	line-height: normal;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: cover;
	font-size: 24 rpx;
	display: flex;
	flex-direction: column;
	/*align-items: self-end;*/
	justify-items: space-between;
	text-align: center;
}

.coupons-price {
	/*font-size: 40px;*/
	/*margin-bottom: 12rpx;*/
}
.coupons-price .price {
	font-size: 80rpx;
}
.coupons-price .title {
	font-size: 36rpx;
	margin-top: 20rpx;
}

.coupons-price > view {
	display: inline-block;
}

.coupons-price .fold {
	font-size: 13px;
}
.maxdiscount {
	font-size: 20rpx;
}

.weui-media-box__desc {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
}

.weui-media-box__title {
	font-size: 12px;
	margin: 15px 0;
}

.coupons-time {
	font-size: 9px;
	color: #333333;
}

.weui-media-box__bd .weui-media-box__desc {
	height: 45rpx;
}

.coupons-bth .bth {
	width: 130rpx;
	padding: 6rpx 0;
	height: 45rpx;
	font-size: 12px;
	text-align: center;
	color: #1bbc9b;
	border: 1px solid #1bbc9b;
	border-radius: 4px;
	margin-right: 8px;
}

.weui-media-box__info {
	padding: 5px 0;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	margin-top: 14rpx;
	font-size: 10px;
	border-top: 1px dashed #f5f5f5;
	color: #666666;
}

.weui-media-box__info .xyicon {
	color: #666666;
}

.weui-panel__ft view {
	font-size: 9px;
	color: #333333;
}

.weui-bar__item_on view {
	color: #1bbc9b;
}

.weui-navbar__slider {
	background-color: #1bbc9b;
	width: 96px;
}

.overdue .weui-media-box__bd view {
	color: #cccccc;
}

.overdue .weui-media-box__hd {
	color: #ffffff;
	/*height: auto;*/
	width: 245rpx;
	line-height: normal;
	background-image: url('data:image/png;base64,');
}
.overdue .xyicon {
	color: #cccccc;
}
.overdue .weui-panel__ft {
	color: #cccccc;
}

